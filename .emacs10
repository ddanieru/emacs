(defun mia-python ()
  (interactive)
  (color-theme-install
   '(mia-python
      ((background-color . "#000000")
      (background-mode . light)
      (border-color . "#030202")
      (cursor-color . "#8cfc4f")
      (foreground-color . "#fafafa")
      (mouse-color . "black"))
     (fringe ((t (:background "#030202"))))
     (mode-line ((t (:foreground "#ffffff" :background "#575753"))))
     (region ((t (:background "#5b5d5b"))))
     (font-lock-builtin-face ((t (:foreground "#e43f45"))))
     (font-lock-comment-face ((t (:foreground "#92968d"))))
     (font-lock-function-name-face ((t (:foreground "#f65a9f"))))
     (font-lock-keyword-face ((t (:foreground "#2e93ff"))))
     (font-lock-string-face ((t (:foreground "#e9b258"))))
     (font-lock-type-face ((t (:foreground"#86d737"))))
     (font-lock-variable-name-face ((t (:foreground "#ffff66"))))
     (minibuffer-prompt ((t (:foreground "#729fcf" :bold t))))
     (font-lock-warning-face ((t (:foreground "Red" :bold t))))
     )))
(provide 'mia-python)
(require 'color-theme)
(color-theme-initialize)
(mia-python)

